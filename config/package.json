{
  "name": "web-weaver-lightning-day10",
  "version": "10.0.0",
  "description": "Day 10 AI Engine v1 - Enterprise-grade JSON extraction achieving 80%+ accuracy with confidence scoring",
  "main": "src/background.js",
  "scripts": {
    "test": "node testing/run-tests.js",
    "test:accuracy": "node testing/run-tests.js --accuracy",
    "test:watch": "nodemon --watch testing --watch src testing/run-tests.js",
    "test:verbose": "node testing/run-tests.js --verbose",
    "test:day10": "node testing/run-tests.js --day10",
    "test:confidence": "node testing/run-tests.js --confidence",
    "test:batch": "node testing/run-tests.js --batch",
    "test:live": "node testing/run-tests.js --live",
    "lint": "eslint src/ testing/",
    "lint:fix": "eslint src/ testing/ --fix",
    "format": "prettier --write 'src/**/*.js' 'testing/**/*.js' 'config/**/*.json'",
    "format:check": "prettier --check 'src/**/*.js' 'testing/**/*.js' 'config/**/*.json'",
    "build": "npm run lint && npm run format:check && npm run test:accuracy",
    "build:day10": "npm run lint:fix && npm run format && npm run test:day10",
    "validate": "node testing/validate-day10.js",
    "analytics": "node testing/analytics-report.js",
    "clean": "rm -rf testing/logs/*.csv testing/logs/*.json",
    "docs": "node scripts/generate-docs.js",
    "deploy": "npm run build:day10 && npm run validate"
  },
  "keywords": [
    "chrome-extension",
    "ai",
    "json-extraction",
    "web-scraping",
    "day10",
    "confidence-scoring",
    "enterprise",
    "80-percent-accuracy",
    "gemini-api",
    "data-quality",
    "pii-stripping",
    "date-standardization"
  ],
  "author": "Web Weaver Lightning Team",
  "license": "MIT",
  "type": "commonjs",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/web-weaver-lightning-day10.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/web-weaver-lightning-day10/issues"
  },
  "homepage": "https://github.com/your-username/web-weaver-lightning-day10#readme",
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@eslint/json": "^0.13.2",
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "nodemon": "^3.1.0",
    "jest": "^29.7.0",
    "puppeteer": "^21.5.0"
  },
  "dependencies": {
    "acorn": "^8.15.0",
    "acorn-jsx": "^5.3.2",
    "ajv": "^6.12.6",
    "chalk": "^4.1.2",
    "argparse": "^2.0.1",
    "ansi-styles": "^4.3.0",
    "balanced-match": "^1.0.2",
    "brace-expansion": "^1.1.12",
    "callsites": "^3.1.0",
    "color-convert": "^2.0.1",
    "color-name": "^1.1.4",
    "concat-map": "^0.0.1",
    "cross-spawn": "^7.0.6",
    "debug": "^4.4.3",
    "deep-is": "^0.1.4",
    "escape-string-regexp": "^4.0.0",
    "fast-csv": "^5.0.1",
    "date-fns": "^2.30.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  },
  "config": {
    "day10": {
      "accuracyTarget": 80,
      "confidenceThreshold": 50,
      "enablePIIStripping": true,
      "enableDateStandardization": true,
      "enableAutoCorrect": true,
      "maxRetries": 3,
      "tabTimeout": 6000
    }
  }
}
